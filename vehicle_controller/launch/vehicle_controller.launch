<!-- vehicle_controller.launch -->
<launch>

	<!-- Vehicle Controller -->
	<node pkg="vehicle_controller" type="vehicle_controller_node" name="vehicle_controller_node" output="screen">
		<remap from="cmd_vel" 		to="cmd_vel"/>
		<param name="teleop/linear"		type="double"	value="40.0"/>
		<param name="teleop/angular"	type="double"	value="10.0"/>
		
		<param name="wheel_radius"		type="double"	value="0.024"/>
		<param name="tread_width"		type="double"	value="0.150"/>

		<param name="debug/enable"		type="bool"		value="false"/>
	</node> 
</launch>

